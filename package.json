	"private": true,
	"scripts": {
		"android": "react-native run-android",
		"ios": "react-native run-ios",
		"iphone": "react-native run-ios --simulator=\"iPhone 14 (17.4)\"",
		"iphone15": "react-native run-ios --simulator=\"iPhone 15 (17.4)\"",
		"ipad": "react-native run-ios --simulator=\"iPad Air (5th generation) (17.0)\"",
		"clean:ios": "rm -f ios/Podfile.lock && rm -rf ios/Pods && rm -rf ios/build",
		"clean:android": "rm -rf android/app/build && rm -rf android/.gradle",
		"clean:gradle": "cd android && ./gradlew --stop && ./gradlew clean && ./gradlew cleanBuild && cd ..",
		"clean:yarn": "rm -rf node_modules/ && rm -f yarn.lock",
		"lint": "eslint .",
		"start": "react-native start",
		"test": "jest",
		"type-check": "tsc",
		"test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
		"pod:install": "cd ios && bundle exec pod install && cd ..",
		"postinstall": "npx patch-package",
		"storybook-generate": "sb-rn-get-stories"
	},
